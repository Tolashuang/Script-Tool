import cv2

vc = cv2.VideoCapture('E:/video2photo/1.mp4')
c = 1

if vc.isOpened():
    rval, frame = vc.read()
else:
    rval = False

timeF = 15

while rval:
    rval, frame = vc.read()
    if (c % timeF == 0):
        cv2.imwrite('E:/video2photo/' + str(c) + '.jpg', frame) 
    c = c + 1
    cv2.waitKey(1)
vc.release()
